{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "jest/globals": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "react/display-name": 0,
    "no-useless-constructor": 2
  }
}
